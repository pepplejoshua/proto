@use("next");
// constants
a : int : 1;
b :: a + 2;

// variables
c := b + a + 4;

// functions
main :: fn () -> void {
  age :: next.forward(23);
  name :: "joshua";
  josh := user.(name, age);
}

// struct type
user :: struct {
  name : str;
  age : int;

  get_name :: fn () -> str {
    return name;
  }
}

// code module
maths :: mod {
  add :: fn (a : int, b : int) -> int {
    return a + b;
  }

  sub :: fn (a : int, b : int) -> int {
    return a - b;
  }
}