fn main() -> str {
    mut name: str = "";
    for i in "joshua" {
        name += i;
        println(i);
    }
    
    
    mut sum = 0;
    let range = 1..=20;
    for n in range {
        sum += n;
    }

    let a = Test { a: 3 };
    mut b = a;
    b.a += 3;
    println(a.a);
    stringf("The sum of {#} is {#}.", range, sum)
}

