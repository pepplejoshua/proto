fn print(i: i64) {}

let cond = true;
mut a = 0;
while cond {
    a += 1;

    if a > 5 {
        break;
    }
}

for mut i = 0; i < 3; i += 1 {
    print(i);
}

let arr = [1, 2, 3, 4, a];
for item in arr {
    print(item);
}

mut i = 0;
loop {
    i += 1;
    if i >= 6 {
        break;
    }
}

i = 0;
loop {
    for i in arr {
        print(i);        
        continue;
    }
    i += 1;

    if (i % 2 == 0) && i > 15 {
        break;
    }
}