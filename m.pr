struct A {
    a : int

    fn init(aa int) void {
        a = aa
    }
}

fn main() int {
    int_s_ptr := new([int])
    defer free(int_s_ptr)
    a_ptr := new(A, 3)
    defer free(a_ptr)
    int_ptr := new(int)
    defer free(int_ptr)

    println(`int_s_ptr := {int_s_ptr}`)
    println(`*int_s_ptr = {*int_s_ptr}`)

    return 0
}
