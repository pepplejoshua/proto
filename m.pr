fn main() int {
    defer {
    }
    a := Node(100);
    s :: "a is " + a.as_str() + ".";
    println(s);
    return 0;
}

struct Node {
    n : int;

    fn init(num int) void {
        self.n = num;
    }

    fn as_str() str {
        return `Node {{ n: {self.n} }`;
    }

    fn update(new_n int) void {
        n = new_n;
    }
}
