struct A {
    num : int

    fn init(n int) void {
        self.num = n
    }

    fn update(n int) void {
        self.num = n
    }

    const fn as_str() str {
        return `A {{ num: {self.num} }`;
    }
}


fn main() int {
    a :: new(A, 300); defer free(a)
    println(a.as_str())

    aa := A(120)
    aa.update(1)
    println(aa.as_str())

    return 0
}
