fn main() int {
    arr :: [1, 2, 3, 4, 5]

    for (i : uint = 0; i < arr.len(); i = i + 1) {
        defer println(`now incrementing i which is currently {i}.`)
        if i == 2 {
            println("skipping on i = 2"); continue
        }
        n :: arr[i]
        println(`i is {i} and n is {n}`)
    }

    return 0
}
