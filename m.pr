fn main() int {
  // type annotated with size
  a : [3, bool] : [false, false, true];
  // infer size with type annotation
  b : [_, i8] : [1, 2, 3];
  // infer size and type
  c : [_, _] : ["abc", "def", "ghi"];
  // no type at all
  d :: ['a', 'e', 'i', 'o', 'u'];

  // error
  e :: [true, false, 'c', "str"];

  return 0;
}
