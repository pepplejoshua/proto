fn main() int {
  // type annotated with size
  a : [3, bool] : [false, false, true];
  // infer size and type
  b : [_, str] : ["abc", "def", "ghi"];
  // no type at all
  c :: ['a', 'e', 'i', 'o', 'u'];
  // array of array
  d : [3, [2, int]] = [[1, 2], [3, 4], [5, 6]];


  // error
  e :: [true, false, 'c', "str"];

  return 0;
}
